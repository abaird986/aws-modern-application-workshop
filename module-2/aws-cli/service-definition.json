{
  "serviceName": "MythicalMysfits-Service",
  "cluster": "MythicalMysfits-Cluster",
  "launchType": "FARGATE",
  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 75
  },
  "desiredCount": 1,
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "assignPublicIp": "DISABLED",
      "securityGroups": [
        "REPLACE_ME_SG_ID"
      ],
      "subnets": [
        "REPLACE_ME_PUBLIC_SUBNET_ONE",
        "REPLACE_ME_PUBLIC_SUBNET_TWO"
      ]
    }
  },
  "taskDefinition": "mythicalmysfitsservice",
  "loadBalancers": [
    {
      "containerName": "MythicalMysfits-Service",
      "containerPort": 8080,
      "targetGroupArn": "REPLACE_ME_NLB_TARGET_GROUP_ARN"
    }
  ]
}